{
  "name": "mapplz",
  "description": "Simple geodata and mapmaking",
  "main": "./lib/mapplz",
  "keywords": [
    "map",
    "etl",
    "geo",
    "gis",
    "postgis",
    "spatial"
  ],
  "version": "0.0.11",
  "author": "mapmeld",
  "repository": {
    "type": "git",
    "url": "https://github.com/mapmeld/mapplz-node.git"
  },
  "dependencies": {
    "coffee-script": "1.7.x",
    "geolib": "2.0.x",
    "pg": "3.4.x",
    "mongodb": "1.4.x",
    "fast-csv": "0.4.x"
  },
  "devDependencies": {
    "coffeelint": "1.5.x",
    "should": ">= 0.0.1",
    "mocha": "1.18.x",
    "chai": "1.9.x",
    "supertest": "0.12.x"
  },
  "engines": {
    "node": ">= 0.10.x",
    "npm": ">= 1.2.x"
  },
  "scripts": {
    "test": "npm run test-coffeelint && npm run test-mocha",
    "test-mocha": "mocha --compilers coffee:coffee-script/register",
    "test-coffeelint": "coffeelint -f test/coffeelint.json ./lib/*.coffee ./test/*.coffee"
  },
